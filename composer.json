{
    "name": "dinophp/dinophp",
    "description": "DinoPHP is a web application framework with expressive. We believe development must be an enjoyable and creative experience. DinoPHP takes the pain out of development by easing common tasks used in many web projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mohamed Ibrahim",
            "email": "eng.ahmedmohamed.2002@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "filp/whoops": "^2.14",
        "rakit/validation": "^1.4",
        "vlucas/phpdotenv": "^5.3",
        "dinophp/bubbletemplate" : "1.0.1",
        "phpunit/phpunit": "9.5.7",
        "friendsofphp/php-cs-fixer": "3.0.0"
    },
    "autoload" : {
        "psr-4": {
            "DinoPHP\\" : "src/",
            "App\\" : "app/"
        },
        "files": [
            "src/Helpers/helper.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
